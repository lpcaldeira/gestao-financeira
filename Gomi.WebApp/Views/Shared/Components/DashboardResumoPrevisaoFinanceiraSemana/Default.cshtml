@model Gomi.Infraestrutura.Models.Financeiro.Dashboard.PrevisaoFinanceiraSemana;

<h6 style="color:purple;font-size:12px;"><b> Previsões Financeiras para esta Semana:</b></h6>


    <h6 style="color:black;font-size:12px;">  @(Model.DataIncialSemana.ToShortDateString()) a @(Model.DataFinalSemana.ToShortDateString()) </h6>


    <h6 style="color:green;font-size:12px;"><b> Contas a Receber: <font style="color:black"> @(Model.Pagar)</font> </b></h6>
    <h6 style="color:red;font-size:12px;"><b> Contas a Pagar: <font style="color:black"> @(Model.Receber)</font></b></h6>
    <h6 style="color:black;font-size:12px;"><b> Diferença:<font style="color:black"> @(Model.Diferenca)</font></b></h6>



